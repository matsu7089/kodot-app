---
import ListLayout from '@/layouts/ListLayout.astro'
import { customPaginatePaths, getTags } from '@/utils'

export const getStaticPaths = customPaginatePaths('articles')

const { page } = Astro.props
const tags = await getTags('articles')
---

<ListLayout
  primaryColor="blue"
  title="Articles"
  icon="book-blue"
  page={page}
  tags={tags}
/>
